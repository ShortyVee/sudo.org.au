{
  "name": "sudo-website",
  "version": "1.0.0",
  "description": "Website Source for http://sudo.org.au",
  "main": "gulpfile.js",
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "cross-env": "^5.0.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
